    	<dependency org="log4j" name="log4j" rev="1.2.17" conf="runtime"/>
		<dependency org="org.slf4j" name="slf4j-log4j12" rev="1.7.21" conf="runtime"/>
		<dependency org="org.slf4j" name="jcl-over-slf4j" rev="1.7.21" conf="runtime"/>
