<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	
	<definition name="defaultLayout" template="/WEB-INF/layouts/default.jsp" />
	
	<definition name="default" extends="defaultLayout">
		<put-attribute name="navloc" value="home" type="string"/>
		<put-attribute name="head" value="/WEB-INF/jsp/default-head.jsp" />
		<put-attribute name="header" value="/WEB-INF/jsp/default-header.jsp"/>
		<put-attribute name="footer" value="/WEB-INF/jsp/default-footer.jsp"/>
	</definition>
	
	<definition name="ajaxLayout" template="/WEB-INF/layouts/ajax.jsp"/>
	
</tiles-definitions>
