<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
	"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	
	<definition name="importLayout" extends="default">
		<put-attribute name="navloc" type="string" value="import"/>
		<put-attribute name="head" value="/WEB-INF/jsp/sec/import/head.jsp" />
	</definition>
	
	<definition name="import/*" extends="importLayout">
		<put-attribute name="body" value="/WEB-INF/jsp/sec/import/{1}.jsp" />
	</definition>
	
</tiles-definitions>
