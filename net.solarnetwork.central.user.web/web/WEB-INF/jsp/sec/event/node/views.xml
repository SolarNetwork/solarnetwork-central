<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
	"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	
	<definition name="eventNodeLayout" extends="default">
		<put-attribute name="navloc" type="string" value="node-event"/>
		<put-attribute name="head" value="/WEB-INF/jsp/sec/event/node/head.jsp" />
	</definition>
	
	<definition name="event/node/*" extends="eventNodeLayout">
		<put-attribute name="body" value="/WEB-INF/jsp/sec/event/node/{1}.jsp" />
	</definition>
	
</tiles-definitions>
