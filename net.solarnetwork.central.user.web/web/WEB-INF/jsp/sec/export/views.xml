<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
	"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
	
	<definition name="exportLayout" extends="default">
		<put-attribute name="navloc" type="string" value="export"/>
		<put-attribute name="head" value="/WEB-INF/jsp/sec/export/head.jsp" />
	</definition>
	
	<definition name="export/*" extends="exportLayout">
		<put-attribute name="body" value="/WEB-INF/jsp/sec/export/{1}.jsp" />
	</definition>
	
</tiles-definitions>
