{
  "name": "solarnet-plv8",
  "description": "SolarNet JavaScript functions for plv8",
  "version": "0.1.0",
  "keywords": [
    "solarnetwork",
    "solarnet"
  ],
  "author": "Matt Magoffin <matt@solarnetwork.net>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "node cmd.js src/**/*.js",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "coverage": "./node_modules/.bin/nyc --reporter=html ./node_modules/.bin/ava"
  },
  "ava": {
    "source": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.22.0",
    "braces": "^2.3.1",
    "lodash": "^4.17.15",
    "lodash.merge": "^4.6.2",
    "glob": "^7.1.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "csv": "^1.1.1",
    "moment": "^2.17.1",
    "nyc": "^14.1.1",
    "sinon": "^1.17.7"
  }
}
