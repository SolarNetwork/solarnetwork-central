{
  "name": "solarnet-plv8",
  "description": "SolarNet JavaScript functions for plv8",
  "version": "0.1.0",
  "keywords": [
    "solarnetwork",
    "solarnet"
  ],
  "author": "Matt Magoffin <matt@solarnetwork.net>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "node cmd.js src/**/*.js",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "coverage": "./node_modules/.bin/nyc --reporter=html ./node_modules/.bin/ava"
  },
  "ava": {
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.5",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "glob": "^7.1.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "csv": "^1.1.1",
    "moment": "^2.17.1",
    "nyc": "^14.1.1",
    "sinon": "^1.17.7"
  }
}
